{
  "name": "@folio/remote-sync",
  "version": "1.1.0",
  "description": "FOLIO app for Remote System Sync",
  "main": "src/index.js",
  "repository": "folio-org/mod-remote-sync",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "lint": "eslint .",
    "test": "yarn run test:unit",
    "test:jest": "jest --ci --coverage",
    "test:unit": "yarn run test:jest && yarn run test:unit:report",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "test:unit:report": "cp -r ./artifacts/coverage-jest ./artifacts/coverage"
  },
  "stripes": {
    "actsAs": [
      "app",
      "handler",
      "settings"
    ],
    "displayName": "ui-remote-sync.meta.title",
    "handlerName": "eventHandler",
    "route": "/remote-sync",
    "home": "/remote-sync",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "remote-sync": "1.0"
    },
    "stripesDeps": [
      "@folio/stripes-erm-components"
    ],
    "icons": [
      {
        "name": "app",
        "alt": "FOLIO app for remote sync",
        "title": "Remote Sync"
      }
    ],
    "permissionSets": [
      {
        "permissionName": "module.remote-sync.enabled",
        "displayName": "UI: ui-remote-sync module is enabled"
      },
      {
        "permissionName": "settings.remote-sync.enabled",
        "displayName": "Settings (remote-sync): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ]
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@folio/eslint-config-stripes": "^6.3.0",
    "@folio/stripes": "^8.0.0",
    "@folio/stripes-cli": "^2.6.1",
    "@folio/stripes-erm-components": "^8.0.0",
    "@folio/stripes-erm-testing": "^1.0.0",
    "@folio/stripes-testing": "^4.2.0",
    "@formatjs/cli": "^4.2.31",
    "@interactors/html": "^1.0.0-rc1.2",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.3.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "jest-junit": "^13.2.0",
    "jest-webpack": "^0.5.1",
    "moment": "^2.22.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-query": "^3.9.0",
    "react-router-dom": "^5.2.0",
    "sinon": "^9.0.2",
    "@folio/handler-stripes-registry": "^1.0.0"
  },
  "dependencies": {
    "@k-int/stripes-kint-components": "^4.0.0",
    "classnames": "^2.2.6",
    "compose-function": "^3.0.3",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "query-string": "^6.6.0",
    "react-final-form": "^6.4.0",
    "react-final-form-arrays": "^3.1.1",
    "react-xarrows": "1.7.0"
  },
  "peerDependencies": {
    "@folio/handler-stripes-registry": "^1.0.0",
    "@folio/stripes": "^8.0.0",
    "@folio/stripes-erm-components": "^8.0.0",
    "moment": "^2.22.2",
    "react": "*",
    "react-dom": "*",
    "react-intl": "^5.8.1",
    "react-query": "^3.9.0",
    "react-router-dom": "^5.2.0"
  }
}
